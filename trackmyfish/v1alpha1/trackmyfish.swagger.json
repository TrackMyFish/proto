            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "TrackMyFishService"
        ]
      }
    }
  },
  "definitions": {
    "FishGender": {
      "type": "string",
      "enum": [
        "UNSPECIFIED",
        "MALE",
        "FEMALE"
      ],
      "default": "UNSPECIFIED"
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "v1alpha1AddFishResponse": {
      "type": "object",
      "properties": {
        "fish": {
          "$ref": "#/definitions/v1alpha1Fish",
          "title": "The added fish"
        }
      }
    },
    "v1alpha1AddTankStatisticResponse": {
      "type": "object",
      "properties": {
        "tank_statistic": {
          "$ref": "#/definitions/v1alpha1TankStatistic",
          "title": "The added tank statistic"
        }
      }
    },
    "v1alpha1DeleteFishResponse": {
      "type": "object",
      "properties": {
        "fish": {
          "$ref": "#/definitions/v1alpha1Fish",
          "title": "The deleted fish"
        }
      }
    },
    "v1alpha1DeleteTankStatisticResponse": {
      "type": "object",
      "properties": {
        "tank_statistic": {
          "$ref": "#/definitions/v1alpha1TankStatistic",
          "title": "The deleted tank statistic"
        }
      }
    },
    "v1alpha1Fish": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int32",
          "description": "The unique identifier of the fish.",
          "readOnly": true
        },
        "type": {
          "type": "string",
          "title": "The type of the fish (e.g. Gourami)",
          "readOnly": true
        },
        "subtype": {
          "type": "string",
          "title": "The subtype of the fish (e.g. Pearl)",
          "readOnly": true
        },
        "color": {
          "type": "string",
          "title": "The color of the fish"
        },
        "gender": {
          "$ref": "#/definitions/FishGender",
          "title": "The gender of the fish"
        },
        "purchase_date": {
          "type": "string",
          "title": "The date of purchase of the fish"
        },
        "count": {
          "type": "integer",
          "format": "int32",
          "title": "The number of fish matching this description"
        }
      }
    },
    "v1alpha1HeartbeatResponse": {
      "type": "object"
    },
    "v1alpha1ListFishResponse": {
      "type": "object",
      "properties": {
        "fish": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1alpha1Fish"
          },
          "title": "The list of fish"
        }
      }
    },
    "v1alpha1ListTankStatisticsResponse": {
      "type": "object",
      "properties": {
        "tank_statistics": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1alpha1TankStatistic"
          },
          "title": "The list of tank statistics"
        }
      }
    },
    "v1alpha1TankStatistic": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int32",
          "description": "The unique identifier of the tank statistic.",
          "readOnly": true
        },
        "test_date": {
          "type": "string",
          "title": "The date of tank test"
        },
        "ph": {
          "type": "number",
          "format": "float"
        },
        "gh": {
          "type": "number",
          "format": "float"
        },
        "kh": {
          "type": "number",
          "format": "float"
        },
        "ammonia": {
          "type": "number",
          "format": "float"
        },
        "nitrite": {
          "type": "number",
          "format": "float"
        },
        "nitrate": {
          "type": "number",
          "format": "float"
        },
        "phosphate": {
          "type": "number",
          "format": "float"
        }
      }
    }
  }
}
